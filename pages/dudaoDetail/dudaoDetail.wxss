.phb {
  padding: 42rpx 40rpx 2rpx;
  background: #fff;
}

.phb .tit {
  font-size: 34rpx;
  color: rgba(22, 22, 22, 1);
  line-height: 48rpx;
  margin-bottom: 27rpx;
}

.phb .tit image {
  margin-right: 20rpx;
  width: 31rpx;
  height: 26rpx;
}

.phb .tabs {
  height: 37rpx;
  margin-bottom: 20rpx;
  margin-top: 36rpx;
}

.phb .tabs .tab {
  display: inline-block;
  font-size: 26rpx;
  color: rgba(160, 160, 160, 1);
  line-height: 37rpx;
  margin-right: 29rpx;
  vertical-align: top;
  position: relative;
}

.phb .tabs .tab .line {
  width: 1rpx;
  height: 15rpx;
  background: rgba(202, 202, 202, 1);
  position: absolute;
  right: -13rpx;
  top: 13rpx;
}

.phb .list {
}

.phb .list .card {
  height: 40rpx;
  margin-bottom: 23rpx;
}

.phb .list .card>view {
  vertical-align: top;
}

.phb .list .card .c-left {
  display: inline-block;
  width: 253rpx;
  line-height: 40rpx;
  height: 40rpx;
  font-size: 28rpx;
  color: rgba(77, 77, 77, 1);
}

.phb .list .card .c-right {
  display: inline-block;
  border-radius: 4rpx;
  background: rgba(243, 247, 248, 1);
  width: 415rpx;
  height: 40rpx;
  text-align: justify;
}

.phb .list .card .c-right  text {
  font-size: 24rpx;
  color: rgba(100, 100, 100, 1);
  line-height: 40rpx;
  display: inline-block;
  vertical-align: top;
}

.phb .list .card .c-right  text:nth-of-type(1) {
  width: 87rpx;
  text-align: right;
}

.phb .list .card .c-right  text:nth-of-type(2) {
  width: 247rpx;
  text-align: center;
}

.phb .list .card .c-right  text:nth-of-type(3) {
  width: 95;
  text-align: left;
}

